<h1>New trip_feature</h1>

<% form_for(@trip_feature) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :trip %><br />
    <%= f.text_field :trip %>
  </p>
  <p>
    <%= f.label :type %><br />
    <%= f.text_field :type %>
  </p>
  <p>
    <%= f.label :kind %><br />
    <%= f.text_field :kind %>
  </p>
  <p>
    <%= f.label :desc %><br />
    <%= f.text_area :desc %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', trip_features_path %>